<div class="container">
  <div class="row">
    <div class="col-12">
      <h2 class="head-line text-center title_h2">配送先編集</h2>
      <%= form_with model: @delivery_place, url: delivery_place_path, method: :patch, local: true do |f| %>
        <div class="col-10">
          <div class="form-group col-12">
            <%= f.label :"郵便番号(ハイフンなし)",class: "col-3 control-label" %>
            <%= f.text_field :postcode , class: "col-2 form-control-static postcode" %>
          </div>
          <div class="form-group col-12">
            <%= f.label :"住所" ,class: "col-3 control-label" %>
            <%= f.text_field :address ,class: "col-7 form-control-static address" %>
          </div>
          <div class="form-group col-12">
            <%= f.label :"宛名" ,class: "col-3 control-label" %>
            <%= f.text_field :addressee , class: "col-4 form-control-static addressee" %>
          </div>
          <div class="form-group col-12 text-right">
            <%= f.submit '編集する' ,class: "col-2 btn btn-primary" %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>